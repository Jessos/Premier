    <validationRules>
        <fullName>Date_Needed_Not_Today_or_Tomorrow</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Request_Reason__c, &quot;Report/Dashboard Request/Update&quot;) &amp;&amp;
 ISPICKVAL( Status__c , &quot;New&quot;) &amp;&amp;
 OR(Date_Needed__c = TODAY(),Date_Needed__c =  TODAY() + 1)</errorConditionFormula>
        <errorDisplayField>Date_Needed__c</errorDisplayField>
        <errorMessage>Report Requests require more than 2 days notice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Needed_for_Report_Requests</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL(Date_Needed__c) &amp;&amp;
 ISPICKVAL(Request_Reason__c, &quot;Report/Dashboard Request/Update&quot;)&amp;&amp;
 NOT(ISPICKVAL( Priority__c , &quot;Low&quot;))</errorConditionFormula>
        <errorDisplayField>Date_Needed__c</errorDisplayField>
        <errorMessage>Medium/High Priority Report Requests require Date Needed to be filled out</errorMessage>
    </validationRules>
