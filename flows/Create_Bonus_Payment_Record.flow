            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Markup_Percentage__c</field>
            <value>
                <elementReference>Markup</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Pay_Code__c</field>
            <value>
                <elementReference>payCodeId</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Placement__c</field>
            <value>
                <elementReference>selectedPlacementId</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Recruiter_Notes__c</field>
            <value>
                <elementReference>Recruiter_Notes</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Status__c</field>
            <value>
                <stringValue>Pending Approval</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Timesheet__c</field>
            <value>
                <elementReference>Timesheet_Period_Ending</elementReference>
            </value>
        </inputAssignments>
        <object>Additional_Payment__c</object>
    </recordCreates>
    <recordLookups>
        <name>Lookup_Employee</name>
        <label>Lookup Employee</label>
        <locationX>20</locationX>
        <locationY>116</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Lookup_Pay_Code</targetReference>
        </connector>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>employeeId</elementReference>
            </value>
        </filters>
        <object>Contact</object>
        <outputAssignments>
            <assignToReference>employeeFirstName</assignToReference>
            <field>FirstName</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>employeeLastName</assignToReference>
            <field>LastName</field>
        </outputAssignments>
    </recordLookups>
    <recordLookups>
        <name>Lookup_Pay_Code</name>
