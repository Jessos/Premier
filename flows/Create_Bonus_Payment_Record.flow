<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <dynamicChoiceSets>
        <name>picklistTimesheets</name>
        <dataType>String</dataType>
        <displayField>tc9_ti__End_Date__c</displayField>
        <filters>
            <field>tc9_ti__End_Date__c</field>
            <operator>GreaterThanOrEqualTo</operator>
            <value>
                <elementReference>last2Months</elementReference>
            </value>
        </filters>
        <filters>
            <field>tc9_ti__Placement__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>selectedPlacementId</elementReference>
            </value>
        </filters>
        <filters>
            <field>tc9_ti__Status__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Rejected</stringValue>
            </value>
        </filters>
        <object>tc9_ti__Time_Sheet__c</object>
        <sortField>tc9_ti__End_Date__c</sortField>
        <sortOrder>Asc</sortOrder>
        <valueField>Id</valueField>
    </dynamicChoiceSets>
    <formulas>
        <name>billableAmount</name>
        <dataType>Currency</dataType>
        <expression>Round({!Employee_Bonus_Amount}*(1+({!Markup}/100)),2)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>last2Months</name>
        <dataType>Date</dataType>
        <expression>TODAY()-61</expression>
    </formulas>
    <interviewLabel>Create Bonus Payment Record {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Create Bonus Payment Record</label>
    <processType>Flow</processType>
    <recordCreates>
        <name>Create_Additional_Payment</name>
        <label>Create Additional Payment</label>
        <locationX>182</locationX>
        <locationY>117</locationY>
        <inputAssignments>
            <field>Billable_Amount__c</field>
            <value>
                <elementReference>billableAmount</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Employee_Amount__c</field>
            <value>
                <elementReference>Employee_Bonus_Amount</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Employee__c</field>
