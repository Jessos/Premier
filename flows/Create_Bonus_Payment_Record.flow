            <value>
                <elementReference>selectedPlacementId</elementReference>
            </value>
        </filters>
        <object>ts2__Placement__c</object>
        <outputAssignments>
            <assignToReference>employeeId</assignToReference>
            <field>ts2__Employee__c</field>
        </outputAssignments>
    </recordLookups>
    <screens>
