  </apex:outputPanel>
  
  <script>premierAPI.footer();</script>
  
  <apex:outputPanel rendered="{!$CurrentPage.Name == 'tc9_ti__FillTimesheet'}">
      <script>
          var startText = $(".ftsHelpContentTxt:eq(0)").text();
          var splitText = startText.split('\n');
          
          $(".ftsHelpContentTxt:eq(0)").text("");
          for(i = 0; i < splitText.length; i++) {
              $(".ftsHelpContentTxt:eq(0)").append('<p>' + splitText[i] + '</p>');
          }
          
          $(".ftsHelpContentTxt:eq(1)").hide();
          $(".ftsHelpContentTxt:eq(2)").hide();
          $("#thePage\\:j_id1\\:j_id23").hide();
      </script>
  </apex:outputPanel>
  
  <apex:outputPanel rendered="{!$CurrentPage.Name == 'ts2__registerconfirmation'}">
