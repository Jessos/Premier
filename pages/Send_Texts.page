<apex:page standardController="Screening_Interview__c" recordSetVar="con" standardStylesheets="false">
	<apex:pageMessages />
	<apex:includeLightning />
	<div id="lightning" />
	<script>
	var IdList = '{!selected}';
	var objectName="Screening_Interview__c";
	var mobileFields="Mobile__c";//If you want to enter multiple fields then please seperate by semi-colon.
	var optOutField="";
	var isOptOut=true;
	var nameField = "Candidate_First_Name__c";
	var unrelatedObjects = "Contact;ts2__Job__c;ts2__Placement__c";//If you want to enter multiple objects then please seperate by semi-colon.
	$Lightning.use("smagicinteract:BulkSMSApp",function() {
		$Lightning.createComponent("smagicinteract:BulkSMSComponent",{
			"IdList"				: IdList,
			"objectName"			: objectName,
			"mobileFields"		: mobileFields,
			"optOutField"		: optOutField,
			"optOutCheckBox"		: isOptOut,
			"nameField"			: nameField,
			"unrelatedObjects"	: unrelatedObjects
		},"lightning",function(cmp) {});
	});
	</script>
</apex:page>